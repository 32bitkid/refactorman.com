<!DOCTYPE html>
<html>
<head>
	<title>
	<% if (page.title) { %>
	<%= page.title %> &mdash; <%= config.title %>
	<% } else { %>
	<%= config.title %> &mdash; <%= config.subtitle %>
	<% } %>
	</title>
	<link rel="icon" href="http://refactorman.com/favicon.ico">
	<meta charset='utf-8'>
	<meta name='viewport' content="width=device-width, initial-scale=1">
	<%- css('css/style.css') %>
	<% if(page.css) { %>
	<%- css(page.css) %>
	<% } %>
</head>
<body>
	<%- body %>
	<% if (page && page.js) { %>
	<%- js(page.js) %>
	<% } %>
</body>
</html>
